set -ue
test_setup

unset EMU_DEBUG
cd $EMU_TEST_DIR/test-source
emu init -v

rm -fv output
echo "touch output"
touch output
echo "emu sink -q add origin $EMU_TEST_DIR/test-sink &> output"
emu sink -q add origin $EMU_TEST_DIR/test-sink &> output
assert_file_is_empty output

rm -fv output
echo "touch output"
touch output
echo "emu sink -q rm origin &> output"
emu sink -q rm origin &> output
assert_file_is_empty output

test_teardown