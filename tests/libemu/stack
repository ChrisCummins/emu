set -e
test_setup
source /usr/local/lib/libemu
cd $EMU_TEST_DIR/test-source

emu init -v
emu stack -v add test ../test-stack

print_stack_details test > tmp

assert_file_is_not_empty tmp

cat <<EOF > tmp2
* test
  Path:           $EMU_TEST_DIR/test-stack
  Size:           8.0K
  Last snapshot:  
  Snapshot count: 0 (0%)
EOF

assert_files_match tmp tmp2

test_teardown
