set -e
test_setup
source /usr/local/lib/libemu
cd /tmp/emu/test-source

emu init -v
emu sink -v add test ../test-sink

print_sink_details test > tmp

assert_file_is_not_empty tmp

cat <<EOF > tmp2
* test
  Path:           /tmp/emu/test-sink
  Size:           8.0K
  Last snapshot:  
  Snapshot count: 0 (0%)
EOF

assert_files_match tmp tmp2

test_teardown
