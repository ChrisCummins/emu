set -ex
test_setup

sink=sink
link=broken-link

emu init
mkdir -pv $sink
emu sink add origin $sink

cd $sink

ln -s /broken/file/path $link
assert_is_broken_link "$link"

set +e
emu clean -n > tmp
set -e
assert_is_broken_link "$link"
grep $link tmp

set +e
emu clean
set -e
assert_does_not_exist "$link"

test_teardown
