set -eux
test_setup

tmp=$EMU_TEST_DIR/normal
tmp2=$EMU_TEST_DIR/debug

rm -fv $tmp
touch $tmp
EMU_DEBUG= emu help clean &>> $tmp

rm -fv $tmp2
touch $tmp2
EMU_DEBUG=1 emu help clean &>> $tmp2

if [[ "$(wc -l $tmp2 | awk '{ print $1 }')" -le "$(wc -l $tmp | awk '{ print $1 }')" ]]
then

    exit 5
fi

test_teardown
