set -eux
test_setup

lock=.emu/LOCK
stack=stack

emu init
echo -e "DATE: $(date)\nPID:  $$" > $lock
assert_is_file $lock

set +e
emu clean -n
set -e
assert_is_file $lock

set +e
emu clean
set -e
assert_is_not_file $lock

mkdir -pv $stack
emu stack add origin $stack
cd $stack
echo -e "DATE: $(date)\nPID:  $$" > $lock
assert_is_file $lock

set +e
emu clean -n
set -e
assert_is_file $lock

set +e
emu clean
set -e
assert_is_not_file $lock

test_teardown
