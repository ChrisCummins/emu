set -ex
test_setup

sink=sink

emu init
mkdir -pv $sink
emu sink add origin $sink

cd $sink

mkdir .emu/trees/new
touch .emu/trees/new/test
assert_is_dir .emu/trees/new
assert_is_file .emu/trees/new/test

set +e
emu clean -n
set -e
assert_is_dir .emu/trees/new
assert_is_file .emu/trees/new/test

set +e
emu clean
set -e
assert_is_not_dir .emu/trees/new
assert_is_not_file .emu/trees/new/test

test_teardown
