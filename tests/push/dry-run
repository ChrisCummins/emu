set -eu
test_setup

cd $EMU_TEST_DIR/test-source

emu init -v
emu sink -v add origin ../test-sink
assert_zero "$(ls ../test-sink/.emu/nodes | wc -l)"
assert_zero "$(ls ../test-sink/.emu/trees | wc -l)"
emu push -n
assert_zero "$(ls ../test-sink/.emu/nodes | wc -l)"
assert_zero "$(ls ../test-sink/.emu/trees | wc -l)"

test_teardown
