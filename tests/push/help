set -eu
test_setup

rm -fv $EMU_TEST_DIR/help "$EMU_TEST_DIR"/help2
touch $EMU_TEST_DIR/help
emu help push >> $EMU_TEST_DIR/help
assert_file_is_not_empty $EMU_TEST_DIR/help

touch $EMU_TEST_DIR/help2
emu push --help >> $EMU_TEST_DIR/help2
assert_file_is_not_empty $EMU_TEST_DIR/help2

assert_files_match $EMU_TEST_DIR/help "$EMU_TEST_DIR"/help2

test_teardown
