set -eu
test_setup

rm -fv $EMU_TEST_DIR/help
touch $EMU_TEST_DIR/help
emu help init >> $EMU_TEST_DIR/help
assert_file_is_not_empty $EMU_TEST_DIR/help

rm -fv $EMU_TEST_DIR/help
touch $EMU_TEST_DIR/help
emu init --help >> $EMU_TEST_DIR/help
assert_file_is_not_empty $EMU_TEST_DIR/help
rm -fv $EMU_TEST_DIR/help

test_teardown
