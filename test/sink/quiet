set -ue
test_setup

cd /tmp/emu/test-source
emu init -v

rm -fv output
echo "touch output"
touch output
echo "emu sink -q add origin /tmp/emu/test-sink &> output"
emu sink -q add origin /tmp/emu/test-sink &> output
assert_file_is_empty output

rm -fv output
echo "touch output"
touch output
echo "emu sink -q rm origin &> output"
emu sink -q rm origin &> output
assert_file_is_empty output

test_teardown