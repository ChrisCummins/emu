set -eu
test_setup

cd $EMU_TEST_DIR/test-source

emu init
emu stack add origin ../test-stack

emu push > ../tmp
emu push -v > ../tmp2

if [[ "$(wc -l ../tmp2 | awk '{ print $1 }')" -le "$(wc -l ../tmp | awk '{ print $1 }')" ]]
then
    echo "Output not verbose!"
    echo "Standard:"
    cat ../tmp
    echo "Verbose:"
    cat ../tmp2
    exit 5
fi

test_teardown
